page {
  background-color: #f1f1ff;
}

.header {
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  background-color: #fff;
  width: 100%;
  height: 100rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
  box-sizing: border-box;
}

.publish-container {
  width: 68rpx;
  height: 68rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.publish-container .icon-fabu {
  font-size: 48rpx;
}

.search-container {
  flex: 1;
}

.bloglist {
  margin-top: 120rpx;
}

.blog {
  margin-bottom: 20rpx;
  box-sizing: border-box;
}

.nomore {
  text-align: center;
  padding: 24rpx;
  color: #999;
}